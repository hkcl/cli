{
  "name": "oclif",
  "description": "oclif: create your own CLI",
  "version": "1.0.0",
  "author": "Jeff Dickey @jdxcode",
  "oclif": {
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "bin": "oclif"
  },
  "bin": "./bin/run",
  "bugs": "https://github.com/oclif/cli/issues",
  "dependencies": {
    "@oclif/command": "^1.2.22",
    "@oclif/config": "^1.3.45",
    "@oclif/errors": "^1.0.1",
    "@oclif/plugin-help": "^0.7.2",
    "@oclif/plugin-not-found": "^0.1.20",
    "debug": "^3.1.0",
    "fixpack": "^2.3.1",
    "lodash": "^4.17.5",
    "sort-pjson": "^1.0.2",
    "yeoman-environment": "^2.0.5",
    "yeoman-generator": "^2.0.2",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^0.3.10",
    "@oclif/tslint": "^0.2.7",
    "@types/lodash": "^4.14.102",
    "@types/read-pkg": "^3.0.0",
    "@types/shelljs": "^0.7.8",
    "@types/yeoman-generator": "^2.0.1",
    "@types/yosay": "^0.0.29",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-oclif": "^1.3.2",
    "execa": "^0.9.0",
    "fancy-test": "^1.0.1",
    "fs-extra": "^5.0.0",
    "globby": "^7.1.1",
    "mocha": "^5.0.0",
    "npm-run-path": "^2.0.2",
    "nps": "^5.7.1",
    "nps-utils": "^1.5.0",
    "shelljs": "^0.8.1",
    "tmp": "^0.0.33",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib",
    "/templates"
  ],
  "homepage": "https://github.com/oclif/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "oclif/cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "nps lint",
    "postpublish": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest && oclif-dev readme",
    "test": "nps test"
  },
  "types": "lib/index.d.ts"
}
